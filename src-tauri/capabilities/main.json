{
  "identifier": "main",
  "local": true,

  "windows": ["main"],

  "window": [
    {
      "label": "main",
      "file_drop": false
    }
  ],

  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "cmd": "",
          "name": "binaries/yt-dlp",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "shell:default",
    "dialog:default"
  ]
}
